<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="stylesheet" href="/style.css" type="text/css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css">
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <title>330 Management</title>
</head>

<body>
  <div class="container">
    <i class="material-icons menu" style="color: white">menu</i>
    <h6 class="inventario">Inventario</h6>
    <i class="material-icons" id="crop" style="color: white">crop_free</i>
    <div id="searcher"></div>
    <!-- <%= typeof msg != 'undefined' ? msg : '' %> -->
    <form action="/upload" method="POST" enctype="multipart/form-data">
      <div class="file-field input-field">
        <div>
          <span><i class="material-icons menu" style="color: #F75A05">add</i></span>
          <p id="agregar">Agregar producto</p>
          <input class="z-depth-0" name="myImage" type="file" style="border: none">
        </div>
        <div class="file-path-wrapper">
          <input class="file-path validate z-depth-0" type="text" style="border: none">
        </div>
      </div>

      <button type="submit" class="btn" id="sb">Aceptar</button>
    </form>

    <br>

    
    <br>
    <img
    src="<%= typeof file != 'undefined' ? file : 'data:image/png;base64,' %>"
    class="img-s">
    <!-- <a href="https://icons8.com">Icon pack by Icons8</a> -->
    <h5 id="product"><%= typeof clas != 'undefined' ? clas : '' %></h5>

  </div>

  <script src="https://code.jquery.com/jquery-3.2.1.min.js"
    integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>
</body>

</html>